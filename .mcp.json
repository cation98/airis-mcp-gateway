{
  "mcpServers": {
    "airis-agent": {
      "command": "uvx",
      "args": [
        "--from",
        "git+https://github.com/agiletec-inc/airis-agent",
        "airis-agent-mcp"
      ],
      "env": {}
    },
    "context7": {
      "command": "npx",
      "args": ["-y", "@context7/mcp-server"]
    },
    "filesystem": {
      "command": "npx",
      "args": [
        "-y",
        "@modelcontextprotocol/server-filesystem",
        "/Users/kazuki/github/airis-mcp-gateway"
      ]
    },
    "git": {
      "command": "npx",
      "args": ["-y", "@modelcontextprotocol/server-git"]
    }
  }
}
