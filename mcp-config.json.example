{
  "mcpServers": {
    "airis-agent": {
      "command": "npx",
      "args": [
        "-y",
        "mcp-remote",
        "http://airis-agent:8100/sse",
        "--allow-http"
      ],
      "env": {},
      "enabled": true,
      "mode": "hot"
    },
    "airis-workspace": {
      "command": "uvx",
      "args": [
        "--from",
        "git+https://github.com/agiletec-inc/airis-agent",
        "airis-workspace-mcp"
      ],
      "env": {},
      "enabled": false,
      "mode": "cold"
    },
    "context7": {
      "command": "npx",
      "args": [
        "-y",
        "@upstash/context7-mcp"
      ],
      "env": {},
      "enabled": true,
      "mode": "cold"
    },
    "fetch": {
      "command": "uvx",
      "args": [
        "mcp-server-fetch"
      ],
      "env": {},
      "enabled": true,
      "mode": "cold"
    },
    "filesystem": {
      "command": "npx",
      "args": [
        "-y",
        "@modelcontextprotocol/server-filesystem",
        "/workspace/host"
      ],
      "env": {},
      "enabled": false,
      "mode": "cold"
    },
    "git": {
      "command": "npx",
      "args": [
        "-y",
        "@modelcontextprotocol/server-git"
      ],
      "env": {},
      "enabled": false,
      "mode": "cold"
    },
    "memory": {
      "command": "npx",
      "args": [
        "-y",
        "@modelcontextprotocol/server-memory"
      ],
      "env": {
        "MEMORY_FILE_PATH": "/app/data/memory.json"
      },
      "enabled": true,
      "mode": "cold"
    },
    "mindbase": {
      "command": "npx",
      "args": [
        "-y",
        "mcp-remote",
        "http://mindbase:8001/sse",
        "--allow-http"
      ],
      "env": {},
      "enabled": false,
      "mode": "cold",
      "_comment": "Managed by Docker MCP Gateway via airis-catalog.yaml"
    },
    "airis-mcp-gateway-control": {
      "command": "node",
      "args": [
        "/app/gateway-control/index.js"
      ],
      "env": {
        "API_URL": "http://localhost:8000"
      },
      "enabled": true,
      "mode": "hot"
    },
    "airis-commands": {
      "command": "node",
      "args": [
        "/app/airis-commands/index.js"
      ],
      "env": {
        "MCP_CONFIG_PATH": "/app/mcp-config.json",
        "PROFILES_DIR": "/app/profiles"
      },
      "enabled": true,
      "mode": "hot"
    },
    "sequential-thinking": {
      "command": "npx",
      "args": [
        "-y",
        "@modelcontextprotocol/server-sequential-thinking"
      ],
      "env": {},
      "enabled": true,
      "mode": "cold"
    },
    "serena": {
      "profile": "${SERENA_MODE:-serena-remote}",
      "env": {},
      "enabled": true,
      "mode": "cold"
    },
    "tavily": {
      "command": "npx",
      "args": [
        "-y",
        "mcp-remote",
        "https://mcp.tavily.com/mcp/?tavilyApiKey=${TAVILY_API_KEY}"
      ],
      "env": {},
      "enabled": true,
      "mode": "cold"
    },
    "time": {
      "command": "npx",
      "args": [
        "-y",
        "@modelcontextprotocol/server-time"
      ],
      "env": {},
      "enabled": false,
      "mode": "cold"
    },
    "supabase": {
      "command": "npx",
      "args": [
        "-y",
        "@supabase/mcp-server-supabase@latest",
        "--access-token",
        "${SUPABASE_ACCESS_TOKEN}"
      ],
      "env": {},
      "enabled": true,
      "mode": "cold"
    },
    "playwright": {
      "command": "npx",
      "args": [
        "-y",
        "@playwright/mcp@latest"
      ],
      "env": {},
      "enabled": true,
      "mode": "cold"
    },
    "magic": {
      "command": "npx",
      "args": [
        "-y",
        "@21st-dev/magic@latest"
      ],
      "env": {
        "API_KEY": "843946a41d42f0efab2510871985a40cc6d0d4aaca73a0c82252ff313f606419"
      },
      "enabled": true,
      "mode": "cold"
    },
    "morphllm": {
      "command": "npx",
      "args": [
        "-y",
        "@morphllm/morphmcp"
      ],
      "env": {
        "MORPH_API_KEY": "sk-mO-tOX1-qHOuPPK8zY8L7eAM6sDJSNsIB-T0XUvFJ0GgPOls",
        "ENABLED_TOOLS": "edit_file,warpgrep_codebase_search"
      },
      "enabled": true,
      "mode": "cold"
    },
    "chrome-devtools": {
      "command": "npx",
      "args": [
        "-y",
        "chrome-devtools-mcp@latest"
      ],
      "env": {},
      "enabled": true,
      "mode": "cold"
    },
    "stripe": {
      "command": "npx",
      "args": [
        "-y",
        "@stripe/mcp",
        "--tools=all",
        "--api-key",
        "${STRIPE_SECRET_KEY}"
      ],
      "env": {},
      "enabled": false,
      "mode": "cold",
      "_comment": "Set STRIPE_SECRET_KEY in .env"
    },
    "twilio": {
      "command": "npx",
      "args": [
        "-y",
        "@twilio-alpha/mcp"
      ],
      "env": {
        "TWILIO_ACCOUNT_SID": "${TWILIO_ACCOUNT_SID}",
        "TWILIO_API_KEY": "${TWILIO_API_KEY}",
        "TWILIO_API_SECRET": "${TWILIO_API_SECRET}"
      },
      "enabled": false,
      "mode": "cold",
      "_comment": "Create API Key at console.twilio.com > Account > API Keys"
    },
    "cloudflare": {
      "command": "npx",
      "args": [
        "-y",
        "@cloudflare/mcp-server-cloudflare@latest",
        "run"
      ],
      "env": {
        "CLOUDFLARE_API_TOKEN": "${CLOUDFLARE_API_TOKEN}",
        "CLOUDFLARE_ACCOUNT_ID": "${CLOUDFLARE_ACCOUNT_ID}"
      },
      "enabled": false,
      "mode": "cold"
    },
    "github": {
      "command": "npx",
      "args": [
        "-y",
        "@modelcontextprotocol/server-github"
      ],
      "env": {
        "GITHUB_PERSONAL_ACCESS_TOKEN": "${GITHUB_TOKEN}"
      },
      "enabled": true,
      "mode": "cold"
    },
    "postgres": {
      "command": "npx",
      "args": [
        "-y",
        "@modelcontextprotocol/server-postgres",
        "${DATABASE_URL}"
      ],
      "env": {},
      "enabled": false,
      "mode": "cold"
    }
  },
  "profiles": {
    "serena-local": {
      "command": "uvx",
      "args": [
        "--from",
        "git+https://github.com/oraios/serena",
        "serena",
        "start-mcp-server",
        "--transport",
        "stdio"
      ]
    },
    "serena-remote": {
      "command": "npx",
      "args": [
        "-y",
        "mcp-remote",
        "http://serena:8000/sse",
        "--allow-http"
      ]
    }
  },
  "log": {
    "level": "info"
  }
}